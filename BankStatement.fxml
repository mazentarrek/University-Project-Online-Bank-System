<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BankStatementController">
    <children>
        <TableView fx:id="Table" layoutX="14.0" layoutY="89.0" prefHeight="350.0" prefWidth="677.0">
            <columns>
                <TableColumn fx:id="TransactionColumn" prefWidth="226.0" text="Transaction" />
                <TableColumn fx:id="TransactionDescriptionC" minWidth="0.0" prefWidth="456.0" text="Transaction Description" />
            </columns>
        </TableView>
      <Button fx:id="backButton" layoutX="588.0" layoutY="448.0" mnemonicParsing="false" onAction="#backbuttonOnAction" prefHeight="26.0" prefWidth="67.0" style="-fx-background-color: #2859ad;" text="Back" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <AnchorPane layoutX="-5.0" layoutY="-134.0" prefHeight="200.0" prefWidth="721.0" style="-fx-background-color: #2859ad;">
         <children>
              <Label fx:id="StatementLabel" layoutX="242.0" layoutY="151.0" prefHeight="21.0" prefWidth="237.0" text="Statement Of Account" textAlignment="CENTER">
                  <font>
                      <Font size="24.0" />
                  </font>
               <textFill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="BLACK" />
                        <Stop color="BLACK" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </textFill>
              </Label>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
